{
  "name": "@jill64/typed-storage",
  "version": "3.1.2",
  "type": "module",
  "description": "üóÉÔ∏è Type-Safe Web Storage API Wrapper",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/typed-storage/issues",
  "homepage": "https://github.com/jill64/typed-storage#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde/index.d.ts",
      "default": "./dist/serde/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "localstorage",
    "sessionstorage",
    "type-safe",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/typed-storage.git",
    "image": "https://opengraph.githubassets.com/b0bffa22b57cd57379b2a36d7e825c07c28df270985231fe9b3270d2506dc5ed/jill64/typed-storage"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "test": "vitest",
    "prepack": "npm run build",
    "format": "npx prettier -w .",
    "lint": "npx eslint . && npx depcheck"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.1.37",
    "@jill64/prettier-config": "1.0.0",
    "happy-dom": "15.3.0",
    "typescanner": "0.5.3",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "ts-serde": "1.0.7"
  }
}
