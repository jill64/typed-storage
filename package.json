{
  "name": "@jill64/typed-storage",
  "version": "1.0.0",
  "type": "module",
  "description": "Type-safe localStorage wrapper",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/typed-storage/issues",
  "homepage": "https://github.com/jill64/typed-storage#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "localStorage"
  ],
  "repository": {
    "type": "git",
