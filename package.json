{
  "name": "@jill64/typed-storage",
  "version": "0.1.4",
  "type": "module",
  "description": "Type-safe localStorage wrapper",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/typed-storage/issues",
  "homepage": "https://github.com/jill64/typed-storage#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "localStorage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/typed-storage.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm package",
    "preview": "vite preview",
    "test": "playwright test",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "prettier --check . --plugin prettier-plugin-svelte . && eslint ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@sveltejs/package": "2.2.2",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.1",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "publint": "0.2.2",
    "typescript": "5.2.2",
    "@playwright/test": "1.37.1",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.25.0",
    "@types/lodash": "4.14.198",
    "lodash": "4.17.21",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "tslib": "2.6.2",
    "typescanner": "0.5.3",
    "vite": "4.4.9"
  },
  "dependencies": {
    "@jill64/attempt": "^0.1.0"
  }
}
